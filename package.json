{
  "name": "ts-auto-mock",
  "version": "0.0.12",
  "description": "Typescript transformer to unlock automatic mock creation for interfaces and classes",
  "main": "index.js",
  "scripts": {
    "build-index": "webpack --config config/webpack.build.js",
    "build-transformer": "webpack --config config/webpack.build.transformer.js",
    "build": "npm run build-index & npm run build-transformer",
    "compileTransformer": "tsc --lib es6,dom --target ES5 src/transformer/transformer.ts",
    "test": "karma start config/karma/karma.config.js",
    "test-framework": "karma start config/karma/karma.config.framework.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "typescript",
    "spy",
    "mock",
    "auto"
  ],
  "files": [
    "src/mock/*.d.ts",
    "src/mockFactory/*.d.ts",
    "src/mockRepository/*.d.ts",
    "src/transformer/transformer.d.ts",
    "src/transformer/create-mock.ts",
    "index.d.ts",
    "index.js",
    "transformer.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uittorio/ts-auto-mock.git"
  },
  "author": "Vittorio Guerriero",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "3.3.5",
    "@types/node": "10.12.18",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "5.2.1",
    "jasmine": "3.3.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-webpack": "3.0.5",
    "puppeteer": "1.11.0",
    "ts-loader": "5.3.2",
    "ts-node": "7.0.1",
    "webpack": "4.28.3",
    "webpack-cli": "3.1.2",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "url-slug": "2.0.0",
    "path": "0.12.7"
  },
  "peerDependencies": {
    "typescript": "^3.2.2"
  }
}
