{
  "name": "ts-auto-mock",
  "version": "0.0.4",
  "description": "Typescript transformer to unlock automatic mock creation for interfaces and classes",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "webpack --config config/webpack.build.js",
    "compileTransformer": "tsc --lib es6,dom --target ES5 src/transformer/transformer.ts",
    "test": "karma start config/karma/karma.config.js",
    "test-framework": "karma start config/karma/karma.config.framework.js"
  },
  "keywords": [
    "typescript",
    "spy",
    "mock",
    "auto"
  ],
  "files": [
    "dist/index.js",
    "index.d.ts",
    "src/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/uittorio/ts-auto-mock.git"
  },
  "author": "Vittorio Guerriero",
  "license": "ISC",
  "devDependencies": {
    "@types/jasmine": "3.3.5",
    "@types/node": "10.12.18",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "5.2.1",
    "jasmine": "3.3.1",
    "jasmine-ts": "0.3.0",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "2.0.1",
    "karma-webpack": "3.0.5",
    "path": "0.12.7",
    "puppeteer": "1.11.0",
    "source-map-support": "0.5.10",
    "ts-loader": "5.3.2",
    "ts-node": "7.0.1",
    "ts-transformer-keys": "0.3.4",
    "typescript": "3.2.2",
    "url-slug": "2.0.0",
    "webpack": "4.28.3",
    "webpack-cli": "3.1.2",
    "webpack-merge": "4.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {}
}
